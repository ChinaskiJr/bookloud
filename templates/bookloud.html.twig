{% extends 'base.html.twig' %}

{% block content %}
    <h2>Bookloud : {{ keyword.name }}</h2>
    <ul>
        {% for book in keyword.books %}
            <li><a href="/book/{{ book.id }}" class=links>{{ book.editor }} - {{ book.title }} - {{ book.isbn }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}